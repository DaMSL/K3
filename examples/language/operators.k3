// Operator precedences 
// Arithmetic:  {-},{*,/,%}, {+,-}
// String:      {++}
// Comparisons: {<,>,<=,>=}, {==,!=,<>}
// Boolean:     {not}, {and}, {or}

declare x : mut int    = 0
declare y : mut bool   = false
declare z : mut string = ""

trigger t : () = \_ -> (
    x = -5 * 4 + 1 - 3;
    y = (10 < 5 or not 2 > 3 and 1 <= 1) and true and not false or false;
    z = "foo " ++ "bar"
  )

source s1 : () = value ()
feed s1 |> t