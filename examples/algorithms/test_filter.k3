include "Core/Builtins.k3"
include "Annotation/Vector.k3"

declare vec : collection {elem : real } @Vector
declare vec2 : mut collection {elem : real } @Vector

trigger start : ()  = \_ -> (
	vec.insert {elem : 5.0}	;
	vec.insert {elem : 6.0};
	vec.insert {elem : 7.0};
	vec.insert {elem : 10.0};
	
	//vec2 = vec.filter (\i-> (i.elem > 6.0));
	//vec2.iterate (\i -> (print (rtos i.elem)));
	
	(vec.filter (\i-> (i.elem > 6.0))).iterate (\i -> (print (rtos i.elem)))
)

source s1 : () = value ()
feed s1 |> start
