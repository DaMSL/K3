include "Core/Builtins.k3"
include "Annotation/Vector.k3"

declare r : collection {elem : real } @Vector
declare s : mut collection {elem : real } @Vector
declare diff : mut collection {elem : real } @Vector

trigger start : () = \_ -> (
  r.insert {elem : 4.0};
  r.insert {elem : 5.0};
  
  s=r;  
  r.set 0 {elem : 6.0};
  diff = r.sub s;
  r.inPlaceAdd s;
  print "hello"
)	

source s1 : () = value ()
feed s1 |> start 
