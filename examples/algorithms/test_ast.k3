include "Core/Builtins.k3"
include "Annotation/Vector.k3"

declare rec : {x1: int, y1: real} = {x1: 20, y1: 20.5}

declare rec2 : collection {x1: int, y1: real } @Vector

declare x : mut int = 0

declare gib : int -> int 

declare fib : int -> int = \i -> (
	if (i==1 or i==0)
	then (1)
	else (( fib (i-1) ) + (fib (i-2)))

)
trigger start :  () = \_ -> (
	x= fib 6;
	rec2.insert {x1: 1, y1 : 1.2};
	rec2.insert {x1: 2, y1 : 2.4};
	print (itos x)
)

source s1 : () = value ()
feed s1 |> start
